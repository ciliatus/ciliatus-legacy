    "laravel-echo": "^1.5.3",
    "laravel-mix": "1.7.2",
    "lodash": "^4.17.11",
    "vue": "^2.6.10",
    "vue-i18n": "^8.11.2",
    "vue-peity": "^0.5.0"
  },
  "aliasify": {
